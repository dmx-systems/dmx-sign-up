### DMX Configuration for Docker Environments ###

# LDAP
dmx.ldap.protocol = LDAP
dmx.ldap.server = ldap
dmx.ldap.port = 389
dmx.ldap.manager = cn=admin,dc=example,dc=app
dmx.ldap.password = admin
dmx.ldap.user_base = ou=demo_users,dc=example,dc=app
dmx.ldap.user_attribute = uid
dmx.ldap.user_member_group = 
dmx.ldap.logging = DEBUG
dmx.ldap.user_creation.enabled = true
dmx.ldap.group_base = dc=example,dc=app
